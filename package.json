{
  "name": "replace-lockfile-registry",
  "version": "0.0.1",
  "description": "Replace registry of `package-lock.json`, `yarn.lock` and `pnpm-lock.yaml`.",
  "keywords": [
    "lockfile",
    "npm",
    "pnpm",
    "registry",
    "replace",
    "yarn"
  ],
  "homepage": "https://github.com/zjffun/replace-lockfile-registry#readme",
  "bugs": {
    "url": "https://github.com/zjffun/replace-lockfile-registry/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zjffun/replace-lockfile-registry.git"
  },
  "license": "MIT",
  "author": "Jufeng Zhang <zjffun@gmail.com>",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": "./dist/cli.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "npm run build && npm run test",
    "start": "npm run watch",
    "build": "rimraf dist && tsc",
    "watch": "tsc --watch",
    "test": "NODE_ENV=test TS_NODE_PROJECT=tsconfig.test.json NODE_OPTIONS='--loader ts-node/esm' c8 --reporter=lcov mocha 'tests/**/*.test.ts'",
    "fix-registry": "replace-lockfile-registry --registry https://registry.npmjs.org/"
  },
  "dependencies": {
    "commander": "^9.3.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.6",
    "@types/fs-extra": "^11.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.5.9",
    "c8": "^8.0.1",
    "chai": "^4.3.8",
    "codecov": "^3.8.3",
    "cross-env": "^7.0.3",
    "fs-extra": "^11.1.1",
    "mocha": "^10.2.0",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4"
  }
}